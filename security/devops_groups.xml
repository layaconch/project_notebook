<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Groups -->
    <record id="group_devops_user" model="res.groups">
        <field name="name">DevOps User</field>
        <field name="category_id" ref="base.module_category_devops"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_devops_manager" model="res.groups">
        <field name="name">DevOps Manager</field>
        <field name="category_id" ref="base.module_category_devops"/>
        <field name="implied_ids" eval="[(4, ref('group_devops_user'))]"/>
    </record>
</odoo>
